{
	"streamDescriptors": [{
		"name": "cloudsearchDocumentGeneration",
		"partitionScheme": "!{timestamp:yyyy-MM-dd}",
		"format": "JSON",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900
	},
	{
		"name": "fileDownloads",
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"tableDescriptor": {
			"name": "fileDownloadsRecords",
			"columns": {
				"userId": "bigint",
				"timestamp": "timestamp",
				"projectId": "bigint",
				"fileHandleId": "string",
				"associateType": "string",
				"associateId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			}
		}
	},
	{
		"name": "fileUploads",
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"tableDescriptor": {
			"name": "fileUploadsRecords",
			"columns": {
				"userId": "bigint",
				"timestamp": "timestamp",
				"projectId": "bigint",
				"fileHandleId": "string",
				"associateType": "string",
				"associateId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			}
		}
	},
	{
		"name": "fileHandleAssociations",
		"partitionScheme": "dt=!{timestamp:yyyy-MM}",
		"bucket": "${stack}.filehandles.sagebase.org",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 60,
		"backupDisabled": true,
		"tableDescriptor": {
			"name": "fileHandleAssociationsRecords",
			"columns": {
				"timestamp": "timestamp",
				"associateType": "string",
				"associateId": "string",
				"fileHandleId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"dt": "string"
			},
			"parameters": {
				"projection.enabled": "true",
				"projection.dt.type": "date",
				"projection.dt.format": "yyyy-MM",
				"projection.dt.range": "2020-01,NOW",
				"projection.dt.interval": "1",
				"projection.dt.interval.unit": "MONTHS"
			}
		}
	},
	{
		"name": "fileDownloadsTest",
		"devOnly": true,
		"parameterizeDestinationByStack": true,
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 60,
		"tableDescriptor": {
			"name": "fileDownloadsRecordsTest",
			"columns": {
				"userId": "bigint",
				"timestamp": "timestamp",
				"projectId": "bigint",
				"fileHandleId": "string",
				"associateType": "string",
				"associateId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			}
		}
	}]
}