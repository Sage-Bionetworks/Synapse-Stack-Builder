{
	"streamDescriptors": [{
		"name": "cloudsearchDocumentGeneration",
		"partitionScheme": "!{timestamp:yyyy-MM-dd}",
		"format": "JSON",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900
	},
	{
		"name": "fileDownloads",
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"tableDescriptor": {
			"name": "fileDownloadsRecords",
			"columns": {
				"userId": "bigint",
				"timestamp": "timestamp",
				"projectId": "bigint",
				"fileHandleId": "string",
				"associateType": "string",
				"associateId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			}
		}
	},
	{
		"name": "fileUploads",
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"tableDescriptor": {
			"name": "fileUploadsRecords",
			"columns": {
				"userId": "bigint",
				"timestamp": "timestamp",
				"projectId": "bigint",
				"fileHandleId": "string",
				"associateType": "string",
				"associateId": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			}
		}
	},
	{
		"name": "fileHandleAssociations",
		"partitionScheme": "dt=!{timestamp:yyyy-MM}",
		"bucket": "${stack}.filehandles.sagebase.org",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"backupDisabled": true,
		"tableDescriptor": {
			"name": "fileHandleAssociationsRecords",
			"columns": {
				"timestamp": "timestamp",
				"associateType": "string",
				"associateId": "bigint",
				"fileHandleId": "bigint",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"dt": "string"
			},
			"parameters": {
				"projection.enabled": "true",
				"projection.dt.type": "date",
				"projection.dt.format": "yyyy-MM",
				"projection.dt.range": "2020-01,NOW",
				"projection.dt.interval": "1",
				"projection.dt.interval.unit": "MONTHS"
			}
		}
	},
	{
		"name": "fileHandleData",
		"partitionScheme": "dt=!{timestamp:yyyy-MM}",
		"bucket": "${stack}.filehandles.sagebase.org",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 900,
		"backupDisabled": true,
		"tableDescriptor": {
			"name": "fileHandleDataRecords",
			"columns": {
				"id": "bigint",
				"createdOn": "timestamp",
				"updatedOn": "timestamp",
				"status": "string",
				"isPreview": "boolean",
				"contentSize": "bigint",
				"bucket": "string",
				"key": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"dt": "string"
			},
			"parameters": {
				"projection.enabled": "true",
				"projection.dt.type": "date",
				"projection.dt.format": "yyyy-MM",
				"projection.dt.range": "2020-01,NOW",
				"projection.dt.interval": "1",
				"projection.dt.interval.unit": "MONTHS"
			}
		}
	},
	{
		"name": "testStream",
		"devOnly": true,
		"parameterizeDestinationByStack": true,
		"partitionScheme": "dt=!{timestamp:yyyy-MM}",
		"bucket": "${stack}.testdata.sagebase.org",
		"format": "PARQUET",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 60,
		"tableDescriptor": {
			"name": "testStreamRecords",
			"columns": {
				"id": "bigint",
				"timestamp": "timestamp",
				"someField": "string",
				"stack": "string",
				"instance": "string"
			},
			"partitionKeys": {
				"dt": "string"
			},
			"parameters": {
				"projection.enabled": "true",
				"projection.dt.type": "date",
				"projection.dt.format": "yyyy-MM",
				"projection.dt.range": "2020-01,NOW",
				"projection.dt.interval": "1",
				"projection.dt.interval.unit": "MONTHS"
			}
		}
	},
	{
		"name": "aclSnapshots",
		"partitionScheme": "year=!{timestamp:yyyy}/month=!{timestamp:MM}/day=!{timestamp:dd}",
		"bucket": "${stack}.xschildw.snapshots.sagebase.org",
		"format": "JSON",
		"bufferFlushSize": 64,
		"bufferFlushInterval": 180
	}]
}