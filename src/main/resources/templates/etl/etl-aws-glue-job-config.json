{
	"etlDescriptors":[{
		"name": "ProcessAccessRecord",
		"description": "This job processes the access records",
		"scriptLocation": "aws-glue.sagebase.org/scripts/",
		"scriptName": "process_access_record.py",
		"sourcePath" : "log.sagebase.org/accessRecord/",
		"destinationPath" : "log.sagebase.org/processedAccessRecord/",
		"destinationFileFormat" : "json",
		"tableDescriptor": {
			"name": "ProcessedAccessRecord",
			"columns": {
				"SESSION_ID": "string",
				"TIMESTAMP": "bigint",
				"USER_ID": "int",
				"METHOD": "string",
				"REQUEST_URL": "string",
				"USERAGENT": "string",
				"HOST": "string",
				"ORIGIN": "string",
				"X_FORWARDED_FOR": "string",
				"VIA": "string",
				"THREAD_ID": "int",
				"ELAPSE_MS": "int",
				"SUCCESS" : "boolean",
				"STACK": "string",
				"INSTANCE": "string",
				"DATE": "date",
				"VM_Id": "string",
				"RETURN_OBJECT_ID": "string",
				"QUERY_STRING": "string",
				"RESPONSE_STATUS": "int",
				"OAUTH_CLIENT_ID": "string",
				"BASIC_AUTH_USERNAME": "string",
				"AUTHENTICATION_METHOD": "string",
				"NORMALIZED_METHOD_SIGNATURE": "string",
				"CLIENT": "string",
				"CLIENT_VERSION": "string",
				"ENTITY_ID": "int",
				"DATE_TIME": "timestamp"
			},
			"partitionKeys": {
				"year": "string",
				"month": "string",
				"day": "string"
			},
			"parameters": {
				"projection.enabled": "true",
				"projection.year.type": "integer",
				"projection.year.range": "2022,9999",
				"projection.year.interval": "4",
				"projection.month.type": "integer",
				"projection.month.range": "1,12",
				"projection.month.interval": "2",
				"projection.day.type": "integer",
				"projection.day.range": "1,31",
				"projection.day.interval": "2"
			}
		}
	}]
}