{
  "empty": {},
  "dev101portalAppConfigApp": {
    "Type": "AWS::AppConfig::Application",
    "Properties": {
      "Name": "dev-101-portalAppConfigApp"
    }
  },
  "dev101portalAppConfigEnvironment": {
    "Type": "AWS::AppConfig::Environment",
    "Properties": {
      "ApplicationId": {
        "Ref": "dev101portalAppConfigApp"
      },
      "Name": "dev-101-portal"
    }
  },
  "dev101portalAppConfigConfigProfile": {
    "Type": "AWS::AppConfig::ConfigurationProfile",
    "Properties": {
      "ApplicationId": {
        "Ref": "dev101portalAppConfigApp"
      },
      "LocationUri": "hosted",
      "Name": "dev-101-portal-configurations"
    }
  },
  "dev101portalAppConfigHostedConfiguration": {
    "Type": "AWS::AppConfig::HostedConfigurationVersion",
    "Properties": {
      "ApplicationId": {
        "Ref": "dev101portalAppConfigApp"
      },
      "ConfigurationProfileId": {
        "Ref": "dev101portalAppConfigConfigProfile"
      },
      "Description": "Freeform config for toggling experimental mode features",
      "Content": "{\"Provenance v2 visualization\": true}",
      "ContentType": "application/json"
    }
  },
  "Outputs": {
    "dev101portalAppConfigApplicationId": {
      "Description": "The ID of the dev101portal AppConfig Application",
      "Value": {"Ref": "dev101portalAppConfigApp"},
      "Export": {"Name": {"Fn::Sub": "dev101portalAppConfigApplicationId"}}
    },
    "dev101portalAppConfigEnvironmentId": {
      "Description": "The ID of the dev101portal AppConfig Environment",
      "Value": {"Ref": "dev101portalAppConfigEnvironment"},
      "Export": {"Name": {"Fn::Sub": "dev101portalAppConfigEnvironmentId"}}
    },
    "dev101portalAppConfigConfigurationProfileId": {
      "Description": "The ID of the dev101portal AppConfig Configuration Profile",
      "Value": {"Ref": "dev101portalAppConfigConfigProfile"},
      "Export": {"Name": {"Fn::Sub": "dev101portalAppConfigConfigurationProfileId"}}
    }
  }
}
